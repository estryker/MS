<meta name="description" content="<%= @squeak.text %>"/>
<link rel="image_src" href="<%= "http://maps.googleapis.com/maps/api/staticmap?center=#{@squeak.latitude},#{@squeak.longitude}&zoom=13&size=200x200&maptype=roadmap&markers=color:blue%7Clabel:M%7C#{@squeak.latitude},#{@squeak.longitude}&sensor=true" %>" />
<%= gmaps("map_options" => { "center_latitude" => @squeak.latitude, "center_longitude" => @squeak.longitude, "zoom" => @zoom},"markers" => { "data" => @json}) %>
